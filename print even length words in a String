import java.util.*;
public class Main{
    public static void printWords(String s){
        for(String w : s.split(" ")){
            if(w.length() % 2 == 0)
            System.out.println(w);
        }
    }
    public static void main(String[] args)
    {
        String s="i am Greeks and a Greek";
        printWords(s);
    }
}
output;
am
Greeks
